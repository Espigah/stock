    "gulp-load-plugins": "^0.10.0",
    "gulp-minify-css": "^1.2.0",
    "gulp-ng-annotate": "^1.0.0",
    "gulp-plumber": "^1.0.1",
    "gulp-protractor": "^4.1.1",
    "gulp-rev": "^5.0.1",
    "gulp-rev-replace": "^0.4.2",
