{
  "name": "loginnrouters",
  "scripts": {
    "gulp": "node node_modules/gulp/bin/gulp.js",
    "postinstall": "npm i -g gulp@3.9.1 && npm i -g bower && npm i -g karma && bower install  && npm i -g protractor && webdriver-manager update",
    "build": "gulp",
    "start": "gulp serve",
    "test": "npm run gulp test",
    "e2e": "npm run gulp testE2E",
    "build:prod": "npm run build && npm run docker:build",
    "docker:start": "docker run -it -p 4403:80 login-client",
    "docker:build": "docker build -t login-client ."
